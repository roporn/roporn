<!-- Footer -->
<footer id="footer">
  <!-- container -->
  <div class="container">
    <br />
    <br />
    <%= type == 'article' ? 'Liked this article?' : 'Liked this page?' %>
    <br />
    <span>Share it with others:</span>
    <br />
    <div class="fb-share-button" data-href="<%= dataHref %>" data-layout="box_count" data-size="large">
      <a target="_blank" href="<%= href %>" class="fb-xfbml-parse-ignore">Share</a>
    </div>
  </div>
  <!-- /row -->
</footer>

<% if(!locals.isAmp) { %>
  <script defer async src="/js/app.js"></script>
  
  
  <script>
    (function (i, s, o, g, r, a, m) {
      i['GoogleAnalyticsObject'] = r;
      (i[r] =
      i[r] ||
      function () {
        (i[r].q = i[r].q || []).push(arguments);
      }),
      (i[r].l = 1 * new Date());
      (a = s.createElement(o)), (m = s.getElementsByTagName(o)[0]);
      a.async = 1;
      a.src = g;
      m.parentNode.insertBefore(a, m);
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
    
    ga('create', 'UA-162918443-3', 'auto');
    ga('send', 'pageview');
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '5 seconds');
    }, 5000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '10 seconds');
    }, 10000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '15 seconds');
    }, 15000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '20 seconds');
    }, 20000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '25 seconds');
    }, 20000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '30 seconds');
    }, 30000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '60 seconds');
    }, 60000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '120 seconds');
    }, 120000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '240 seconds');
    }, 240000);
    setTimeout(() => {
      ga('send', 'event', 'Time elapsed', '360 seconds');
    }, 360000);
  </script>
</body>
<% } else { %>
  <amp-analytics type="gtag" data-credentials="include">
    <script type="application/json">
      {
        "vars": {
          "gtag_id": "UA-162918443-3",
          "config": {
            "UA-162918443-3": { "groups": "default" }
          }
        }
      }
    </script>
  </amp-analytics>
<% }  %>



