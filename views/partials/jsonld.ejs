<script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "NewsArticle",
    "mainEntityOfPage": {
      "@type": "WebPage",
      "@id": "https://www.rntricks.com<%= video.url %>"
    },
    "headline": "<%= video.title %>",
    "image": ["https://www.rntricks.com<%= video.thumbnailUrl %>"],
    "datePublished": "<%= video.datePublishedJsonLd %>",
    "dateModified": "<%= video.datePublishedJsonLd %>",
    "author": { "@type": "Organization", "name": "ROPORN" },
    "publisher": {
      "@type": "Organization",
      "name": "ROPORN",
      "url": "https://www.rntricks.com/",
      "logo": { "@type": "ImageObject", "url": "https://www.rntricks.com/img/logo.png" }
    },
    "description": "<%= video.subtitle || (video.body[0].innerText + video.body[1].innerText) %>"
  }
</script>
